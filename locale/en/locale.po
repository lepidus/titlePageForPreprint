msgstr "Publication status"

msgid "plugins.generic.titlePageForPreprint.emptyPublicationStatus"
msgstr "Not informed by the submitting author"

msgid "plugins.generic.titlePageForPreprint.checklistLabel"
msgstr "This preprint was submitted under the following conditions"

